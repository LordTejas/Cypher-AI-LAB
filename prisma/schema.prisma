generator client {
  provider = "prisma-client-js"
  engineType = "library"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  userId String @unique
  username String
  email String
  emailVerified Boolean
  image String
  password String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
